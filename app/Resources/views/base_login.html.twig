<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
    
    <title>Textiles Lolo | Login</title>

    {%  stylesheets 'assets/vendor/bootstrap/dist/css/bootstrap.min.css'
                        'assets/vendor/bootstrap/dist/css/bootstrap-theme.min.css'
                        'assets/self/css/login.css'
            filter='cssrewrite' %}
          <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
      <div class="container-fluid">
      <div class="row">
          <div class="col-lg-12">
           {% block body %}{% endblock %}
          </div>
        </div>          
      </div>
      {% javascripts  'assets/vendor/jquery/dist/jquery.min.js'
                      'assets/vendor/bootstrap/dist/js/bootstrap.min.js'
                      'assets/self/js/jquery-ui.min.js'
       %}
          <script src="{{ asset_url }}"></script>
       {% endjavascripts %}
       {% block javascripts %}{% endblock %}
    </body>
</html>
